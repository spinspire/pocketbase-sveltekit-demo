{
  "name": "PollaminUI",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run typegen && env-cmd vite dev",
    "dev:backend": "cd ../backend && air serve --publicDir=../frontend/build",
    "backend": "cd ../backend && ./pocketbase serve --publicDir=../frontend/build",
    "build": "env-cmd -f .env.production vite build",
    "build:backend": "cd ../backend && go build",
    "typegen": "pocketbase-typegen --db ../backend/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "env-cmd playwright test",
    "preview": "env-cmd vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@types/node": "^18.11.17",
    "env-cmd": "^10.1.0",
    "playwright": "^1.29.1",
    "pocketbase-typegen": "^1.1.2",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "sass": "^1.57.1",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "type": "module",
  "dependencies": {
    "pocketbase": "^0.10.1"
  }
}
