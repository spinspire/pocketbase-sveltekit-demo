{
  "name": "pocketbase-sveltekit-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "cd ../pb && ./pocketbase serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/node": "^20.10.1",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.69.5",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.12"
  },
  "type": "module",
  "dependencies": {
    "pocketbase": "^0.21.0"
  }
}