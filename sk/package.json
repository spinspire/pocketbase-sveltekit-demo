{
  "name": "pocketbase-sveltekit-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "cd ../pb && pocketbase serve --publicDir=../sk/build",
    "build": "vite build --mode=production",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:types": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.16.0",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.1",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.20",
    "pocketbase-typegen": "^1.1.2",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "sass": "^1.69.5",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.1",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^4.5.0"
  },
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "marked": "^12.0.1",
    "playwright": "^1.42.1",
    "pocketbase": "^0.15.2",
    "svelte": "^4.2.8",
    "svelte-markdown": "^0.4.1",
    "svelte-particles": "^2.12.0",
    "tsparticles-slim": "^2.12.0"
  }
}
